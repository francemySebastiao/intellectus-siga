	<parameter name="id_curso" class="java.lang.Integer"/>
	<parameter name="id_ano" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     SUM (EE.CLASSIFICACAO) NOTAFINAL,
     C.NUMERO_CANDIDATO,
     C.NOME,
     CR.DESCRICAO CURSO,
     C.TURNO,
     EE.CODIGO_CANDIDATO,
     (SELECT GRUPO_OWNER FROM T_INSTITUICAO WHERE ID = 2) GRUPO_OWNER,
     (SELECT FOTO FROM T_LOGO_MARCA) FOTO,
     (SELECT ANO_LECTIVO_DESCRICAO FROM t_ano_lectivo WHERE ID = EE.CODIGO_ANO_LECTIVO) ANO_LECTIVO_DESCRICAO
FROM T_CANDIDATO C
     INNER JOIN T_EXAME_ELETRONICO EE ON (EE.CODIGO_CANDIDATO = C.ID)
     INNER JOIN T_CURSO CR ON (CR.ID = C.CODIGO_CURSO)
WHERE
     C.CODIGO_CURSO =  $P{id_curso}  AND
     EE.CODIGO_ANO_LECTIVO =  $P{id_ano} 
GROUP BY
     C.NUMERO_CANDIDATO,
     C.NOME,
     CR.DESCRICAO,
     C.TURNO,
     EE.CODIGO_CANDIDATO,
     EE.CODIGO_ANO_LECTIVO
ORDER BY 5,3]]>
	</queryString>
